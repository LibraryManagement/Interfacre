      <Button layoutX="2.0" layoutY="2.0" mnemonicParsing="false" onAction="#changeSceneToLibrarianView" prefHeight="45.0" prefWidth="81.0" style="-fx-alignment: center; -fx-background-radius: 50px; -fx-border-radius: 50px; -fx-background-color: white; -fx-border-color: #816868;" text="X">
