      <Button layoutX="269.0" layoutY="2.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="81.0" style="-fx-alignment: center; -fx-background-radius: 50px; -fx-border-radius: 50px; -fx-border-color: white; -fx-background-color: #816868;" text="Delete" textFill="WHITE">
